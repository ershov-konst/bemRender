<script data-main="/pagesource/main.js" src="/lib/ext/require.js"></script>
{{? it.preparedJS}}
<script type="text/javascript" src="{{=it.preparedJS}}"></script>
{{?}}
<script type="text/javascript">
   (function(){
      var 
         libs = {{=it.libs}},
         dependencies = {{=it.dependencies}};
      
      require(libs, function(){
         require(dependencies, function(){
            var args = arguments;
            var config = new $br.Config({{=it.config}});
            dependencies.forEach(function(d, i){
               var
                  className = d.replace(/js!/g, ""),
                  elements = document.getElementsByClassName(className);
               for (var e = 0, l = elements.length; e < l; e++){
                  $br.storage.push(new args[i](elements[e], config.get(elements[e].id) || {}));
               }
            });
         })
      });
   })();
</script>